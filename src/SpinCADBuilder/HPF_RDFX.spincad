@name HPF_RDFX
@audioInput input Input
@audioOutput output Output  

equ freq 0.15
@sliderLabel freq Frequency 0.001 0.250 0.15 1000.0 3

equ output Reg0
equ lpf1 Reg1

@isPinConnected Input

rdax input, 1.0
rdfx lpf1, freq
wrax lpf1, -1.0
rdax input, 1.0
wrax output, 0

@setOutputPin Output output
@endif
