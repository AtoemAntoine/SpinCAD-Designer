@name HPF_1P
@audioInput input Input
@audioOutput output Output  

equ freq 0.015
// variable - Name - low - high - multiplier - precision - option 
@sliderLabel freq 'Frequency (Hz)' 40 1500 100.0 100.0 1 LOGFREQ 

equ output Reg0
equ lpf1 Reg1

@isPinConnected Input

rdax input, 1.0
rdfx lpf1, freq
wrax lpf1, -1.0
// do a lowpass filter then subtract that from the input.
// voila high pass filter!
rdax input, 1.0
wrax output, 0

@setOutputPin Output output
@endif
